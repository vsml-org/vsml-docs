---
import { getEntry } from "astro:content";
import EntryLayout from "../layouts/EntryLayout.astro";
import MainLayout from "../layouts/MainLayout.astro";

const howToUseEntry = await getEntry("tutorial", "index");
const { Content, headings } = await howToUseEntry.render();
---

<MainLayout title="チュートリアル">
  <EntryLayout headings={headings}>
    <Content />
  </EntryLayout>
</MainLayout>
